{
  "name": "claude-cli-proxy",
  "version": "0.5.9",
  "description": "HTTP proxy that wraps Claude CLI for OpenClaw",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkirienko/claude-cli-proxy.git"
  },
  "bin": {
    "claude-cli-proxy": "./server.js"
  },
  "scripts": {
    "start": "node server.js",
    "start:debug": "DEBUG=1 node server.js",
    "test": "node --test test/**/*.test.js",
    "test:unit": "node --test test/unit/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage test/**/*.test.js",
    "update": "bash scripts/update.sh"
  }
}
